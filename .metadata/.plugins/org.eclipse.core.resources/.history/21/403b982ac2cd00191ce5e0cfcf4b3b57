SRAM_HandleTypeDef hsram2;

/* USER CODE BEGIN PV */

#define AUDIO_FREQ			48000
#define AUDIOBUF_LENGTH 	4096 * 2

volatile char 		uart_txbuf[50];
volatile char 		uart_rxbuf[10];
volatile int		uart_rx_complete = 0;
volatile int16_t 	audiobuf[AUDIOBUF_LENGTH];
volatile int		audio_measurement_complete = 0;

volatile uint32_t 	dma_audio_data_offset;
volatile uint32_t 	dma_audio_data_remaining;

#define FFT_LENGTH		4096
#define FFT_FORWARD		0
arm_rfft_instance_q15			hrfft;

volatile q15_t		fft_result[FFT_LENGTH*2];
//arm_cfft_radix4_instance_q15	hcfft;

/* USER CODE END PV */

/* Private function prototypes -----------------------------------------------*/
void SystemClock_Config(void);
static void MX_GPIO_Init(void);
static void MX_DMA_Init(void);
static void MX_ADC1_Init(void);
static void MX_ADC2_Init(void);
